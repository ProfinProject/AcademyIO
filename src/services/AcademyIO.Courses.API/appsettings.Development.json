{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "Bff"
    }
  },
  "CourseUrl": "https://localhost:7231",
  "StudentUrl": "https://localhost:7127",
  "PaymentUrl": "https://localhost:5001",

  "MessageQueueConnection": {
    "MessageBus": "host=localhost:5672;username=guest;password=guest;publisherConfirms=true;timeout=10"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../AcademyIO_Courses.db"
  },
  "AppSettings": {
    "JwksUri": "https://localhost:5421/jwks",
    "Issuer": "https://academyIO.com",
    "Audience": "AcademyIO"
  }
}